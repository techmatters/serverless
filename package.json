{
  "name": "serverless",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "jest --verbose",
    "start": "npx tsc && twilio-run --env",
    "watch": "npx tsc && twilio-run start --live",
    "swagger": "redoc-cli bundle -o assets/swagger.html swagger.yaml",
    "lint": "eslint --ext ts .",
    "tr": "twilio-run",
    "tr:deploy": "npx tsc && twilio-run deploy --force",
    "preinstall": "cd ./tech-matters-serverless-helpers-lib && npm ci && npx tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tech-matters/serverless.git"
  },
  "devDependencies": {
    "@types/jest": "^28.1.7",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.184",
    "@types/node-fetch": "^2.6.2",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/twit": "^2.2.30",
    "@types/uuid": "^8.3.4",

    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "eslint": "^8.22.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.4.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "jest-each": "^28.1.3",
    "jsonwebtoken": "^8.5.1",
    "mockdate": "^3.0.5",
    "prettier": "^2.7.1",
