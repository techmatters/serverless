    "twilio-run": "^3.4.3",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@tech-matters/serverless-helpers": "file:./tech-matters-serverless-helpers-lib",
    "@twilio-labs/serverless-runtime-types": "^2.2.3",
    "@twilio/runtime-handler": "1.2.4",
    "aws-sdk": "^2.1199.0",
    "axios": "^0.27.2",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.37",
    "swagger-ui-express": "^4.5.0",
    "twilio": "^3.80.1",
